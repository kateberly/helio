      BLOCK DATA
C
C
C     Galileo IS A PROGRAM FOR METRIC MULTIDIMENSIONAL SCALING
C     USING THE METHOD OF YOUNG AND HOUSEHOLDER(1938) AS MODIFIED
C     BY TORGERSON(1958).  IT IS DESIGNED FOR OVER-TIME ANALYSIS
C     OF ANY NUMBER OF DATASETS.  IT INCLUDES PROCEDURES FOR DESIGNING
C     OPTIMAL PERSUASIVE MESSAGES FOLLOWING WOELFEL ET AL.(1977).
C
      IMPLICIT REAL (A-H,O-Z)
      COMMON/AMGCOM/ST,TG,AMG
      COMMON/AXIS/AX(3)
      COMMON/BASE/MNS,EIG,ROT,RD
      COMMON/CASES/CASES
      COMMON/EXVAL/EXVAL
      COMMON/LFN/LFNI,LFNO,LFNP
      COMMON/STATIC/STATIC
      COMMON/DYNAM/DYNAM
      COMMON/HGTLET/HGTLET
      COMMON/QUE/QUE
      COMMON/XCOR/XCOR
      COMMON/MAINT/MAINV
      COMMON/MAXITR/MAXITR
      COMMON/MAXVAL/MAXVAL
      COMMON/MIDDLE/MIDDLE
      COMMON/ROT/ROTT
      COMMON/XE/XE
      COMMON/YE/YE
      COMMON/ZE/ZE
      COMMON/MSG/MSG(4)
      COMMON/NOADR/NOADR
      COMMON/OPT/OPT
      COMMON/PARM/BOXD,SIZE,MWAVE,TYPE,COLOR,VIEW(4),NAME(2)
      COMMON/PLT/PLT
      COMMON/PLTONL/PLTONL
      COMMON/RAWDAT/IDLEN,PAIRS,FORM
      COMMON/RUN/RUN
      COMMON/SELECT/MSK,CHK,LOGIC
      COMMON/UNMASS/UNMASS
      COMMON/PCT/PCT
      COMMON/ANNOTE/ANNOTE(4)
      COMMON/TOL/TOL
      COMMON/TRF/TRF
      COMMON/AP/AP
      COMMON/THINC/THINC
C
      CHARACTER * 40 RUN
      INTEGER AX,PAIRS,UNMASS
      CHARACTER*20 FORM
      CHARACTER*1 CHK(8)
      INTEGER EIG,ROT,RD
      INTEGER CASES,AMG(4),ST,TG,TRF,QUE,XCOR
      INTEGER STATIC,DYNAM,PLT,PLTONL
      INTEGER TYPE,COLOR,VIEW
      INTEGER OPT(30)
C
      REAL MAXVAL
C
      DATA AMG/4*0/
      DATA AX/3,2,1/
      DATA BOXD/.1/
      DATA COLOR/1/
      DATA DYNAM/0/
      DATA EXVAL/1000./
      DATA HGTLET/0.42/
      DATA IDLEN,PAIRS/8,8/
      DATA FORM /'(8A1,8(2I2,F5.0))'/
cC RZ NOVEMBER 23,1984 CHANGE PUCH UNIT TO 9
      DATA LFNI,LFNO,LFNP/5,6,9/
      DATA LOGIC /0/
      DATA MAINV/0/
      DATA MIDDLE /0/
      DATA MAXITR /500/
      DATA MAXVAL/99999./
      DATA MSK/0/
      DATA MSG/4*0/
c      DATA NAME/'*NO ','NAME'/
      DATA NOADR/0/
      DATA OPT/30*0/
      DATA PCT/-1.0/
      DATA PLTONL/0/
      DATA PLT/0/
      DATA QUE/-1/
      DATA SIZE/.6875/
      DATA STATIC/1/
      DATA AP/1./
      DATA THINC/1./
      DATA TOL/.0001/
      DATA TRF/0/
      DATA TYPE/0/
      DATA UNMASS/0/
      DATA VIEW/4*1/
      DATA XE/1./
      DATA YE/15./
      DATA ZE/50./
      DATA ROTT/-45./
      end